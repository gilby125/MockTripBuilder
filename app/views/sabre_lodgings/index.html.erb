<p id="notice"><%= notice %></p>

<h1>Listing Sabre Lodgings</h1>

<table>
  <thead>
    <tr>
      <th>Email address</th>
      <th>Itinerary reference</th>
      <th>Reference term</th>
      <th>Name</th>
      <th>Confirmation number</th>
      <th>Item name</th>
      <th>Start date time</th>
      <th>End date time</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Street address</th>
      <th>Airport code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sabre_lodgings.each do |sabre_lodging| %>
      <tr>
        <td><%= sabre_lodging.email_address %></td>
        <td><%= sabre_lodging.itinerary_reference %></td>
        <td><%= sabre_lodging.reference_term %></td>
        <td><%= sabre_lodging.name %></td>
        <td><%= sabre_lodging.confirmation_number %></td>
        <td><%= sabre_lodging.item_name %></td>
        <td><%= sabre_lodging.start_date_time %></td>
        <td><%= sabre_lodging.end_date_time %></td>
        <td><%= sabre_lodging.city %></td>
        <td><%= sabre_lodging.state %></td>
        <td><%= sabre_lodging.country %></td>
        <td><%= sabre_lodging.street_address %></td>
        <td><%= sabre_lodging.airport_code %></td>
        <td><%= link_to 'Show', sabre_lodging %></td>
        <td><%= link_to 'Edit', edit_sabre_lodging_path(sabre_lodging) %></td>
        <td><%= link_to 'Destroy', sabre_lodging, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sabre lodging', new_sabre_lodging_path %>
