<p id="notice"><%= notice %></p>

<h1>Listing Rails</h1>

<table>
  <thead>
    <tr>
      <th>Email address</th>
      <th>Itinerary reference</th>
      <th>Reference term</th>
      <th>Name</th>
      <th>Confirmation number</th>
      <th>Item name</th>
      <th>Start date time</th>
      <th>End date time</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Street address</th>
      <th>Zip code</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Context</th>
      <th>Reference</th>
      <th>Start date two</th>
      <th>End date two</th>
      <th>Context two</th>
      <th>Reference two</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rails.each do |rail| %>
      <tr>
        <td><%= rail.email_address %></td>
        <td><%= rail.itinerary_reference %></td>
        <td><%= rail.reference_term %></td>
        <td><%= rail.name %></td>
        <td><%= rail.confirmation_number %></td>
        <td><%= rail.item_name %></td>
        <td><%= rail.start_date_time %></td>
        <td><%= rail.end_date_time %></td>
        <td><%= rail.city %></td>
        <td><%= rail.state %></td>
        <td><%= rail.country %></td>
        <td><%= rail.street_address %></td>
        <td><%= rail.zip_code %></td>
        <td><%= rail.lat %></td>
        <td><%= rail.long %></td>
        <td><%= rail.context %></td>
        <td><%= rail.reference %></td>
        <td><%= rail.start_date_two %></td>
        <td><%= rail.end_date_two %></td>
        <td><%= rail.context_two %></td>
        <td><%= rail.reference_two %></td>
        <td><%= link_to 'Show', rail %></td>
        <td><%= link_to 'Edit', edit_rail_path(rail) %></td>
        <td><%= link_to 'Destroy', rail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rail', new_rail_path %>
